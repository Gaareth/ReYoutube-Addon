function generateComments(comments) {
  let comments_wrapper = document.createElement("div");


    for (let comment of comments) {
        let comment_wrapper = document.createElement("div");
        comment_wrapper.id = "body";
        comment_wrapper.className = "style-scope ytd-comment-renderer";

        let comment_thumbnail = document.createElement("div");
        comment_thumbnail.id = "author-thumbnail";
        comment_thumbnail.className = "style-scope ytd-comment-renderer";

        let comment_main = document.createElement("div");
        comment_main.id = "main";
        comment_main.className = "style-scope ytd-comment-renderer";

            let comment_header = document.createElement("div");
            comment_header.id = "header";
            comment_header.className = "style-scope ytd-comment-renderer";

                let comment_header_author = document.createElement("div");
                comment_header_author.id = "header-author";
                comment_header_author.className = "style-scope ytd-comment-renderer";

                    let comment_header_author_h3 = document.createElement("h3");
                    comment_header_author_h3.className = "style-scope ytd-comment-renderer";
                        let comment_header_author_h3_a = document.createElement("a");
                        comment_header_author_h3_a.id = "author-text";
                        comment_header_author_h3_a.className = "yt-simple-endpoint style-scope ytd-comment-renderer";
                        comment_header_author_h3.appendChild(comment_header_author_h3_a);

                        let comment_header_author_h3_a_span = document.createElement("span");
                        comment_header_author_h3_a_span.className = "style-scope ytd-comment-renderer";
                        comment_header_author_h3_a.appendChild(comment_header_author_h3_a_span);
                        comment_header_author_h3_a_span.innerHTML = comment.user.username;

                    comment_header_author.appendChild(comment_header_author_h3);

                    let comment_header_timestamp = document.createElement("div");
                    comment_header_timestamp.style = "display: block !important;";
                    comment_header_timestamp.className = "published-time-text above-comment style-scope ytd-comment-renderer";
                    comment_header_author.appendChild(comment_header_timestamp);
                        let comment_header_timestamp_a = document.createElement("a");
                        comment_header_timestamp_a.className = "yt-simple-endpoint style-scope yt-formatted-string";
                        comment_header_timestamp_a.innerHTML = comment.created_at[1];
                    comment_header_timestamp.appendChild(comment_header_timestamp_a);

                comment_header.appendChild(comment_header_author);

            let comment_content = document.createElement("span");
            comment_content.innerHTML = comment.comment;

        comment_main.appendChild(comment_header);
        comment_main.appendChild(comment_content);


        comment_wrapper.appendChild(comment_thumbnail);
        comment_wrapper.appendChild(comment_main);
        console.log(comment_wrapper)


        if (comment.replies.length > 0) {
          let comment_replies = document.createElement("div");
          comment_replies.className = "comment-replies";

          comment_replies.appendChild(generateComments(comment.replies));

          comment_wrapper.appendChild(comment_replies);
        }

    comments_wrapper.appendChild(comment_wrapper);
  }
    console.log(comments_wrapper)
    return comments_wrapper;
}